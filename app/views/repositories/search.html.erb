<h1>Search Github Repositories</h1>
<%= form_tag '/search' do %>
  <%= label_tag :query %>
  <%= text_field_tag :query %>
  <%= submit_tag "Search Github!" %>
<% end %>
<div>
  <% if @iems %>
    <h2><%= @name %></h2>
    <p><%= @url %></p>
    <% else %>
    <p><% @error %></p>
    <% end %>

</div>
